{
  "name": "docker",
  "displayName": "Docker Language Basics (built-in)",
  "description": "Provides syntax highlighting and bracket matching in Docker files.",
  "version": "1.79.0",
  "publisher": "vscode",
  "license": "SEE LICENSE IN LICENSE-vscode.txt",
  "engines": {
    "vscode": "*"
  },
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "dockerfile",
        "extensions": [
          ".dockerfile",
          ".containerfile"
        ],
        "filenames": [
          "Dockerfile",
          "Containerfile"
        ],
        "filenamePatterns": [
          "Dockerfile.*",
          "Containerfile.*"
        ],
        "aliases": [
          "Docker",
          "Dockerfile",
          "Containerfile"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dockerfile",
        "scopeName": "source.dockerfile",
        "path": "./syntaxes/docker.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[dockerfile]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-theia/vscode-builtin-extensions"
  },
  "keywords": [
    "Built-in"
  ],
  "preview": false
}