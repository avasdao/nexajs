{
  "name": "scss",
  "displayName": "SCSS Language Basics (built-in)",
  "description": "Provides syntax highlighting, bracket matching and folding in SCSS files.",
  "version": "1.79.0",
  "publisher": "vscode",
  "license": "SEE LICENSE IN LICENSE-vscode.txt",
  "engines": {
    "vscode": "*"
  },
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "scss",
        "aliases": [
          "SCSS",
          "scss"
        ],
        "extensions": [
          ".scss"
        ],
        "mimetypes": [
          "text/x-scss",
          "text/scss"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scss",
        "scopeName": "source.css.scss",
        "path": "./syntaxes/scss.tmLanguage.json"
      },
      {
        "scopeName": "source.sassdoc",
        "path": "./syntaxes/sassdoc.tmLanguage.json"
      }
    ],
    "problemMatchers": [
      {
        "name": "node-sass",
        "label": "Node Sass Compiler",
        "owner": "node-sass",
        "fileLocation": "absolute",
        "pattern": [
          {
            "regexp": "^{$"
          },
          {
            "regexp": "\\s*\"status\":\\s\\d+,"
          },
          {
            "regexp": "\\s*\"file\":\\s\"(.*)\",",
            "file": 1
          },
          {
            "regexp": "\\s*\"line\":\\s(\\d+),",
            "line": 1
          },
          {
            "regexp": "\\s*\"column\":\\s(\\d+),",
            "column": 1
          },
          {
            "regexp": "\\s*\"message\":\\s\"(.*)\",",
            "message": 1
          },
          {
            "regexp": "\\s*\"formatted\":\\s(.*)"
          },
          {
            "regexp": "^}$"
          }
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-theia/vscode-builtin-extensions"
  },
  "keywords": [
    "Built-in"
  ],
  "preview": false
}